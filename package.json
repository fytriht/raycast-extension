{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "fytriht",
  "title": "fytriht",
  "description": "zhi's arsenal",
  "icon": "command-icon.png",
  "author": "fytriht",
  "owner": "fytriht-org",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Reset Setapp Devices",
      "description": "Template with a plain detail view",
      "mode": "view",
      "preferences": [
        {
          "name": "setappToken",
          "description": "Setapp token",
          "type": "password",
          "required": true,
          "title": "Setapp token"
        },
        {
          "name": "setappRefreshToken",
          "description": "Setapp refresh token",
          "type": "password",
          "required": true,
          "title": "Setapp refresh token"
        },
        {
          "name": "setappPassword",
          "description": "Setapp password",
          "type": "password",
          "required": true,
          "title": "Setapp password"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.44.0"
  },
  "devDependencies": {
    "@types/jest": "29.2.4",
    "@types/node": "18.8.3",
    "@types/node-fetch": "2.6.2",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "29.3.1",
    "node-fetch": "2.6.7",
    "prettier": "^2.5.1",
    "ts-jest": "29.0.3",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish",
    "test": "jest"
  }
}
